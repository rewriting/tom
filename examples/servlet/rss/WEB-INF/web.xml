<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <display-name>Rss</display-name>
 <description>Simple RSS 2.0 viewer</description>
 <context-param>
  <param-name>lonchamp</param-name>
  <param-value>lonchamp@loria.fr</param-value>
  <description>Test</description>
 </context-param>
 <servlet>
  <servlet-name>rss</servlet-name>
  <servlet-class>Rss</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>rss</servlet-name>
  <url-pattern>/Rss</url-pattern>
 </servlet-mapping>
 <session-config>
  <session-timeout>30</session-timeout>
 </session-config>
</web-app>
